            "post": {
                "tags": [
                    "Movie Controller"
                ],
                "summary": "Store a movie",
                "description": "Store a movie",
                "operationId": "2c657c385fd3c24a1aa167d69b57c24f",
                "requestBody": {
                    "description": "Recipe data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
